{
  "version": 2,
  "routes": [{ "src": "^/.*", "dest": "/index.html", "status": 200 }],
  "headers": [
    {
      "source": "/(.*).(js|css|json|ico|png|jpg|jpeg|svg|woff|woff2|ttf|otf)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
